import requests
import urllib.parse
import urllib3

urllib3.disable_warnings(urllib3.exceptions.InsecureRequestWarning)

BASE_URL = "https://api.encrypt.wtf/new/api.php"
TOKEN = "ifindy"
BASE = "telefone2"

def consultar_telefone(telefone):
    query = urllib.parse.quote(telefone)
    url = f"{BASE_URL}?token={TOKEN}&base={BASE}&query={query}"

    print(f"üîç Consultando API direta: {url}")

    try:
        resp = requests.get(url, timeout=20, verify=False)
        print(f"‚úÖ Status: {resp.status_code}")
        print(resp.json())
    except Exception as e:
        print(f"‚ùå Erro: {e}")

if __name__ == "__main__":
    tel = input("Digite o telefone com DDD: ").strip()
    consultar_telefone(tel)
